#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_win-detect-browsers@1.0.2@win-detect-browsers/bin/detect-browsers" "$@"
  ret=$?
else 
  node  "$basedir/../../../_win-detect-browsers@1.0.2@win-detect-browsers/bin/detect-browsers" "$@"
  ret=$?
fi
exit $ret
