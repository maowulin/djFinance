#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_default-browser-id@1.0.4@default-browser-id/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_default-browser-id@1.0.4@default-browser-id/cli.js" "$@"
  ret=$?
fi
exit $ret
